  "main": "index.js",
  "name": "hardhat",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NarcoNik/hardhat.git"
  },
  "scripts": {
    "clean": "rm -r node_modules package-lock.json",
    "compile": "npx hardhat compile",
    "deploy:hardhat": "npx hardhat run scripts/deploy.ts --network hardhat ",
    "deploy:local": "npx hardhat run scripts/deploy.ts --network localhost ",
    "format": "npx prettier --write ./ && eslint --fix ./",
    "lint": "npx solhint contracts/*.sol contracts/**/*.sol abstract/*.sol abstract/**/*.sol interfaces/*.sol interfaces/**/*.sol dividend/*.sol  dividend/**/*.sol libs/*.sol  libs/**/*.sol test/**/*.sol",
    "ln": "docker compose --file ./docker-compose.yaml up -d",
    "ln-stop": "docker compose --file ./docker-compose.yaml down -v && docker-compose --file ./docker-compose.yaml rm -v",
    "push": "git add ./ && git commit -m 'refactoring' -a && git push origin",
    "test": "npx hardhat test --network localhost",
    "verify": "npx hardhat run scripts/verify.ts"
  },
  "version": "0.9.7"
}
